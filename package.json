{
  "name": "quicken",
  "version": "0.0.0",
  "description": "Speed up slow-to-start CLI commands",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/quicken",
  "bugs": "https://github.com/vinsonchuong/quicken/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/quicken",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "bin": {
    "quicken": "./quicken.js",
    "quicken-worker": "./quicken-worker.js"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "ndjson": "^1.5.0",
    "node-pty": "^0.9.0",
    "npm": "^6.14.7",
    "p-event": "^4.2.0",
    "resolve": "^1.17.0",
    "wait-on": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^3.10.1",
    "fs-extra": "^9.0.1",
    "quicken": "link:.",
    "semantic-release": "^17.1.1",
    "tempy": "^0.6.0",
    "xo": "^0.32.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "10m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/extensions": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-useless-path-segments": "off",
      "no-await-in-loop": "off",
      "unicorn/import-index": "off"
    }
  },
  "quicken": {
    "xo": [
      "xo/cli-main.js"
    ]
  }
}
